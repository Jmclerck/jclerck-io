{
  "name": "jclerck.io",
  "version": "0.1.0",
  "author": "Jon Clerck <jmclerck@icloud.com>",
  "description": "",
  "keywords": [
    "express"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/Jmclerck/jclerck.io",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "lint": "xo --fix",
    "start": "node dist/index.js",
    "stop": "kill -9 $(ps aux | grep 'node dist/index.js' | awk '{print $2}')",
    "test": "snyk test",
    "watch": "watch 'npm run lint; npm run build; npm run stop; npm run start' src",
    "snyk-monitor": "snyk monitor",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-stage-0": "^6.5.0",
    "greenkeeper": "^3.1.0",
    "watch": "^0.19.1",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "snyk": "^1.17.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-es2015-modules-commonjs"
    ],
    "sourceMaps": "inline"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "babel/object-curly-spacing": [
        1,
        "always"
      ]
    }
  },
  "snyk": true
}
