{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "serviceworker": true
  },
  "rules": {
    "array-bracket-spacing": [1, "never"],
    "arrow-body-style": [2, "as-needed"],
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": [1, {
      "after": true,
      "before": true
    }],
    "block-spacing": [1],
    "camelcase": [1, {
      "properties": "always"
    }],
    "comma-dangle": [2, "always"],
    "comma-spacing": [1],
    "curly": [2, "all"],
    "eol-last": [1],
    "eqeqeq": [2, "smart"],
    "generator-star-spacing": [1, {
      "after": true,
      "before": false
    }],
    "guard-for-in": [1],
    "indent": [1, 2, {
      "SwitchCase": 1
    }],
    "keyword-spacing": [1],
    "key-spacing": [1],
    "max-len": [1, 150],
    "linebreak-style": [1, "unix"],
    "new-parens": [2],
    "no-array-constructor": [2],
    "no-bitwise": [2],
    "no-duplicate-imports": [1],
    "no-eval": [1],
    "no-invalid-this": [2],
    "no-lonely-if": [1],
    "no-mixed-spaces-and-tabs": [1],
    "no-multiple-empty-lines": [1, {
      max: 1
    }],
    "no-nested-ternary": [2],
    "no-new-func": [1],
    "no-new-object": [2],
    "no-new-wrappers": [1],
    "no-plusplus": [1],
    "no-script-url": [2],
    "no-sequences": [2],
    "no-shadow": [2, {
      "builtinGlobals": true,
      "hoist": "all"
    }],
    "no-spaced-func": [1],
    "no-trailing-spaces": [1],
    "no-use-before-define": [2],
    "no-useless-call": [1],
    "no-useless-computed-key": [1],
    "no-useless-constructor": [1],
    "no-useless-rename": [1],
    "no-warning-comments": [1],
    "no-with": [2],
    "object-curly-spacing": [1, "never"],
    "object-property-newline": [2],
    "one-var": [2, "never"],
    "prefer-arrow-callback": [1],
    "prefer-const": [1],
    "prefer-rest-params": [1],
    "prefer-spread": [1],
    "prefer-template": [1],
    "quote-props": [2, "as-needed", {
      "keywords": true
    }],
    "quotes": [2, "single", {
      "avoidEscape": true
    }],
    "radix": [1, "always"],
    "require-yield": [1],
    "semi": [2],
    "semi-spacing": [1],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "always"],
    "space-infix-ops": [1],
    "space-in-parens": [1, "never"],
    "space-unary-ops": [2, {
      "words": true
    }],
    "template-curly-spacing": [1, "never"],
    "yield-star-spacing": [1],
    "yoda": [1, "never"]
  }
}
