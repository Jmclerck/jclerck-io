{
  "name": "jclerck.io",
  "version": "0.1.0",
  "author": "Jon Clerck <jmclerck@icloud.com>",
  "description": "",
  "keywords": [
    "News",
    "Notifications",
    "Offline"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/Jmclerck/jclerck.io",
  "scripts": {
    "babel": "babel src -d dist --source-maps",
    "build": "npm run lint; npm run babel",
    "lint": "xo '**/*.js' --fix",
    "start": "list .",
    "watch": "watch 'npm run lint; npm run build' src"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "watch": "^0.19.1",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "list": "^0.8.2",
    "normalize.css": "^5.0.0",
    "systemjs": "^0.19.39",
    "whatwg-fetch": "^1.0.0"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime", {
          "polyfill": false
        }
      ],
      "transform-es2015-modules-systemjs"
    ],
    "sourceMaps": "inline"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "comma-dangle": [2, "always-multiline"],
      "babel/object-curly-spacing": [1, "always"]
    },
    "global": [
      "Headers",
      "Response",
      "caches",
      "document",
      "fetch",
      "navigator",
      "self"
    ]
  }
}
