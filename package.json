{
  "name": "jclerck.io",
  "version": "0.1.0",
  "author": "Jon Clerck <jmclerck@icloud.com>",
  "description": "Jclerck.io Service Worker enabled Flickr demo",
  "keywords": [
    "Feeds",
    "News",
    "Notifications",
    "Offline",
    "RSS",
    "Really Simple Syndication"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/Jmclerck/jclerck.io",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "lint": "xo --fix",
    "test": "snyk test",
    "watch": "watch 'npm run lint; npm run build' src",
    "snyk-monitor": "snyk monitor",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-stage-0": "^6.5.0",
    "greenkeeper": "^3.1.0",
    "watch": "^0.19.1",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "systemjs": "^0.19.32",
    "whatwg-fetch": "^1.0.0",
    "snyk": "^1.17.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false
        }
      ],
      "transform-es2015-modules-systemjs"
    ],
    "sourceMaps": "inline"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "babel/object-curly-spacing": [
        1,
        "always"
      ]
    },
    "global": [
      "Headers",
      "Response",
      "caches",
      "document",
      "fetch",
      "navigator",
      "self"
    ]
  },
  "snyk": true
}
